{
  "auth": {
    "profiles": {
      "google:default": {
        "provider": "google",
        "mode": "api_key"
      }
    }
  },
  "tools": {
    "profile": "full"
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "google/gemini-2.5-pro",
        "fallbacks": [
          "google/gemini-2.5-flash"
        ]
      },
      "workspace": "/root/.clawdbot"
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "lan",
    "auth": {
      "mode": "token",
      "token": "praticos-secret-token-change-me"
    }
  },
  "channels": {
    "whatsapp": {
      "selfChatMode": false,
      "dmPolicy": "allowlist",
      "allowFrom": [
        "+5548984090709"
      ]
    }
  },
  "plugins": {
    "entries": {
      "whatsapp": {
        "enabled": true
      },
      "telegram": {
        "enabled": false
      }
    }
  },
  "skills": {
    "entries": {
      "praticos": {
        "env": {
          "PRATICOS_API_URL": "${PRATICOS_API_URL}",
          "PRATICOS_API_KEY": "${PRATICOS_API_KEY}"
        }
      }
    }
  }
}
