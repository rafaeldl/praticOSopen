{
  "auth": {
    "profiles": {
      "google:default": {
        "provider": "google",
        "mode": "api_key"
      }
    }
  },
  "tools": {
    "profile": "full",
    "web": {
      "fetch": {
        "enabled": true
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "google/gemini-3-flash-preview",
        "fallbacks": [
          "google/gemini-2.5-flash-lite"
        ]
      },
      "models": {
        "google/gemini-3-flash-preview": {
          "alias": "gemini-flash"
        }
      },
      "workspace": "/app/workspace"
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "lan",
    "auth": {
      "mode": "token",
      "token": "praticos-secret-token-change-me"
    }
  },
  "channels": {
    "whatsapp": {
      "selfChatMode": false,
      "dmPolicy": "allowlist",
      "allowFrom": [
        "+5548984090709"
      ]
    }
  },
  "plugins": {
    "entries": {
      "whatsapp": {
        "enabled": true
      },
      "telegram": {
        "enabled": false
      }
    }
  }
}
