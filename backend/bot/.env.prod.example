# ClawdBot Production Environment Variables
# Copy this file to .env.prod and fill in the values
# NEVER commit .env.prod to git!

# Gemini API Key (Google AI Studio)
GEMINI_API_KEY=your-gemini-api-key-here

# PraticOS API Configuration
PRATICOS_API_URL=https://southamerica-east1-praticos.cloudfunctions.net/api
PRATICOS_API_KEY=bot_praticos_prod_xxx

# ClawdBot Gateway Token
CLAWDBOT_TOKEN=praticos-prod-token-xxx
